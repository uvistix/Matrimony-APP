{
  "expo": {
    "name": "BWB Matrimony",
    "slug": "bwb-matrimony",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6F155C"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6F155C"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.bwbmatrimony.app",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "30df9ba1-fa62-409c-b10f-e516186b697f"
      }
    },
    "ios": {
      "bundleIdentifier": "com.bwbmatrimony.app"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/crashlytics"
    ]
  }
}
